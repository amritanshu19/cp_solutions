#include<bits/stdc++.h>
using namespace std;
 
int main()
{
  int t; cin>>t;
  while(t--)
  {
    int n; cin>>n;
    int a[n], mrq=0;
    for(int i=0; i<n;i++)
    {
      cin>>a[i];
    }
    sort(a, a+n);
    if(n!=1)
    {
    for(int i=0; i<n;i++)
    {
      if(i!=0||i!=n-1)
      {
      if(abs(a[i-1]-a[i])>1&&abs(a[i+1]-a[i])>1)
      {
        mrq++;
      }
      }
      else if(i==0)
      {
        if(abs(a[1]-a[0])>1)
      {
        mrq++;
      }
      }
      else if(i==n-1)
      {
        if(abs(a[n-1]-a[n-2])>1)
      {
        mrq++;
      }
      }
    }
    }
    else if(n==1)
    {
      mrq=0;
    }
    if(mrq>0)
    {
      cout<<"NO\n";
    }
    else
    {
      cout<<"YES\n";
    }
  }
  return 0;
  
}



